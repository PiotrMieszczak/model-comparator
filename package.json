  "scripts": {
    "prepare": "husky",
    "commit": "cz",
    "dev": "bun nx run-many --target=serve --projects=web,api",
    "dev:web": "bun nx serve web",
    "dev:api": "bun nx serve api",
    "build": "bun nx run-many --target=build --projects=web,api",
    "build:web": "bun nx build web",
    "build:api": "bun nx build api",
    "test": "bun nx run-many --target=test --all",
    "test:web": "bun nx test web",
    "test:api": "bun nx test api",
    "lint": "bun nx run-many --target=lint --all",
    "lint:web": "bun nx lint web",
    "lint:api": "bun nx lint api",
    "typecheck": "bun nx run-many --target=typecheck --projects=web,api",
    "typecheck:web": "bun nx typecheck web",
    "typecheck:api": "bun nx typecheck api",
    "start": "bun dev",
    "preview:web": "bun nx preview web"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
